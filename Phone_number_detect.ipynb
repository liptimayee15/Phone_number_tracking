    "import phonenumbers\n",
    "from phonenumbers import geocoder\n",
    "input_number=input(\"Enter phone Number: \")\n",
    "phone_number = phonenumbers.parse(input_number,\"IN\")\n",
    "print(\"Phone number location\")\n",
    "print(geocoder.description_for_number(phone_number,\"en\"));\n"
